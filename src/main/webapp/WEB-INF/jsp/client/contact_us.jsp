<body>
    <%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
           
	<c:import url="/shop/header" charEncoding="utf-8" />        
    
    <section id="main" class="page-contact">
        <div class="container_12">
            <div id="content" class="grid_12">
                <header>
                    <h1 class="page_title">联系我们</h1>
                </header>
                    
                <article>
                    <div class="grid_6 adress">
			<h3>地址</h3>
			<p> 阿奇代尔49, 查尔斯顿2B, 纽约, 美国</p>
			<hr>
			
			<h3>电话</h3>
			<p>技术支持: <span>+777 (100) 1234</span><br>
			销售经理: <span>+777 (100) 4321</span><br>
			人事助理: <span>+777 (100) 1243</span></p>
			<hr>
			
			<h3>电子邮件地址</h3>
			<p>技术支持: <span class="mail">support@example.com</span><br>
			销售经理: <span class="mail">manager@example.com</span><br>
			人事助理: <span class="mail">chief@example.com</span></p>
                    </div><!-- .adress -->
                    
                    <div class="grid_6 form">
			<form class="contact">
			    <h2>快速反馈</h2>
							
			    <div class="name">
				<strong>名字:</strong><sup>*</sup><br>
				<input type="text" name="name" value="">
			    </div><!-- .name -->
				
			    <div class="email">
				<strong>邮件地址:</strong><sup>*</sup><br>
				<input type="email" name="email" value="">
			    </div><!-- .email -->
							
			    <div class="phone">
				<strong>电话:</strong><br>
				<input type="text" name="phone" value="">
			    </div><!-- .phone -->
				
			    <div class="comment">
				<strong>内容:</strong><sup>*</sup><br>
				<textarea name="comment"></textarea>
			    </div><!-- .comment -->
				
			    <div class="submit">
				<div class="field"><sup>*</sup><span>必需字段</span></div>
				<input type="submit" value="Submit">
			    </div><!-- .submit -->
		    </form><!-- .contact -->
                </div><!-- .grid_6 -->
                    
		</article>
                    
                <div class="clear"></div>
            </div><!-- #content -->

            <div class="clear"></div>
        </div><!-- .container_12 -->
    </section><!-- #main -->
    <div class="clear"></div>
        
    <c:import url="footer.jsp" charEncoding="utf-8" /> 
    
